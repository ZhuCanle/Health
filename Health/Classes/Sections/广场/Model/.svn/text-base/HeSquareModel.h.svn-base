#import <Foundation/Foundation.h>
#import "HeSquareCommentModel.h"

@interface HeSquareModel : NSObject

/** 发帖人昵称 */
@property (copy, nonatomic) NSString *user_nickname;
/** 发帖人头像 */
@property (copy, nonatomic) NSString *user_head_img;
/** 发帖人ID */
@property (copy, nonatomic) NSString *user_id;
/** 帖子id */
@property (assign, nonatomic) NSInteger posts_id;
/** 帖子标题 */
@property (copy, nonatomic) NSString *posts_title;
/** 帖子内容 */
@property (copy, nonatomic) NSString *posts_content;
/** 帖子图片 */
@property (strong, nonatomic) NSMutableArray *posts_img;
/** 帖子是否置顶 */
@property (assign, nonatomic) NSInteger posts_top_state;
/** 帖子发布时间 */
@property (copy, nonatomic) NSString *posts_create_time;
/** 标签（可以是多个用逗号分隔） */
@property (copy, nonatomic) NSString *label_name;
/** 评论数组 */
@property (strong, nonatomic) NSMutableArray *commentInfor;

@end
